{
	"servers": {
		"mcp-http-stream-server": {
			"type": "http",
			"url": "http://localhost:8080/mcp/stream",
			"headers": {
				"jira_token": "${input:jira_token}",
				"jira_username": "${input:jira_username}"
			}
		},
		"mcp-emoji-server": {
			"url": "http://localhost:8080/mcp/stream",
			"type": "http"
		}
	},
	"inputs": [
		{
			"type": "promptString",
			"id": "jira_token",
			"description": "Jira api token",
			"password": false
		},
		{
			"type": "promptString",
			"id": "jira_username",
			"description": "Jira username (email)",
			"password": false
		}
	]
}